import java.util.Scanner;


public class Faktorial {
    public static void main (String [] args) {
        System.out.println("Masukkan angka: "+"!");
        Scanner a = new Scanner (System.in);
         int b = a.nextInt();
        for(int i=1;i<=b;i++)
        {
            System.out.print(i+"!");
        }
  
    }
}
